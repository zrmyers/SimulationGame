    "button": [
        {
            "id": "simple",
            "font-id": "Default-UI",
            "disabled": {
                "nine-slice-id": "button-disabled-style",
                "text-color": {
                    "r": 0.9,
                    "g": 0.9,
                    "b": 0.9,
                    "a": 0.9
                }
            },
            "enabled": {
                "nine-slice-id": "button-enabled-style",
